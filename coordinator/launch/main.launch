<?xml version="1.0" encoding="utf-8"?>
<launch>
  <node
	  pkg="beta_navigator"
	  type="beta_navigator"
	  name="$(anon beta_navigator)"
	  output="screen"
	  args="1.3 0 0"
	  />
  <!-- args represent TABLE (x, y, th), in that order -->

  <!-- Insert your nodes here. -->
  
  <node
	  pkg="rviz"
	  type="rviz"
	  name="rviz"
	  args="-d $(find beta_navigator)/launch/main.rviz"
	  />
</launch>
